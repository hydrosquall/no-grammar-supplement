{
  "description": "multivariate normal data",
  "data": {
    "url": "flight_delays2017_data.json"
  },
  "rebin": {
    "type": "square",
    "size": 64,
    "aggregation": "sum"
  },
  "compose": {
    "mix": "blend",
    "mixing": "multiplicative"
  },
  "legend": {
    "title": "CARRIER"
  },
  "rescale": {
    "type": "log"
  },
  "axis": {
    "x": {
      "title": "DISTANCE (miles)"
    },
    "y": {
      "title": "ARR_DELAY (minutes)"
    }
  }
}